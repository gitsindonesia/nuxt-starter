{
  "version": "0.4.0",
  "name": "@morpheme/nuxt-starter",
  "description": "GITS Nuxt Starter",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.16",
    "@morpheme/nuxt": "^1.0.0-beta.6",
    "@pinia/nuxt": "^0.4.7",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "pinia": "^2.0.33"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@babel/core": "^7.21.3",
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@nuxt/image-edge": "1.0.0-27991759.1a5eea1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/fontaine": "^0.2.4",
    "@nuxtjs/google-fonts": "^3.0.0",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/partytown": "^1.3.0",
    "@nuxtjs/tailwindcss": "^6.6.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.16",
    "babel-loader": "^9.1.2",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "nuxt": "^3.3.2",
    "sass": "^1.60.0",
    "standard-version": "^9.5.0",
    "vitest": "^0.28.5",
    "vue-loader": "^17.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css,vue}": "npm run lint:fix"
  },
  "type": "module",
  "main": "./nuxt.config.ts",
  "files": [
    "assets",
    "components",
    "layouts",
    "locales",
    "pages",
    "plugins",
    "public",
    "stores",
    "utils",
    ".eslintignore",
    ".eslintrc",
    "app.config.ts",
    "app.vue",
    "commitlint.config.js",
    "nuxt.config.ts",
    "tailwind.config.js"
  ]
}
