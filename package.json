{
  "name": "@morpheme/nuxt-starter",
  "type": "module",
  "version": "0.5.1",
  "description": "GITS Nuxt Starter",
  "main": "./nuxt.config.ts",
  "files": [
    "assets",
    "components",
    "layouts",
    "locales",
    "pages",
    "plugins",
    "public",
    "stores",
    "utils",
    ".eslintignore",
    ".eslintrc",
    "app.config.ts",
    "app.vue",
    "commitlint.config.js",
    "nuxt.config.ts",
    "tailwind.config.js"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "start": "node .output/server/index.mjs",
    "test:unit": "vitest",
    "test:dev": "NUXT_VITEST_DEV_TEST=true nuxt dev",
    "test": "pnpm test:unit --run && pnpm test:dev",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "@morpheme/nuxt": "^1.0.0-rc.0",
    "@pinia/nuxt": "^0.4.11",
    "@vueuse/core": "^10.3.0",
    "@vueuse/nuxt": "^10.3.0",
    "pinia": "^2.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@babel/core": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nuxt/devtools": "^0.6.7",
    "@nuxt/image-edge": "1.0.0-28020728.5df24eb",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/fontaine": "^0.2.5",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@storybook/addon-actions": "^7.3.0",
    "@storybook/addon-essentials": "^7.3.0",
    "@storybook/addon-interactions": "^7.3.0",
    "@storybook/addon-links": "^7.3.0",
    "@storybook/addon-mdx-gfm": "^7.3.0",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/vue3": "^7.3.0",
    "@storybook/vue3-vite": "^7.3.0",
    "@vitest/coverage-c8": "^0.32.4",
    "babel-loader": "^9.1.3",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "nuxt": "^3.7.1",
    "nuxt-vitest": "^0.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.65.1",
    "standard-version": "^9.5.0",
    "storybook": "^7.3.0",
    "vitest": "^0.32.4",
    "vitest-environment-nuxt": "^0.8.7",
    "vitest-sonar-reporter": "^0.4.1",
    "vue-loader": "^17.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css,vue}": "npm run lint:fix"
  }
}
